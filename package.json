{
  "name": "cler-vision-app",
  "version": "1.0.0",
  "description": "An Electron application with Svelte",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json",
    "svelte-check": "svelte-check --tsconfig tsconfig.web.json",
    "typecheck": "npm run typecheck:node && npm run typecheck:web && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:staging": "electron-vite dev --mode staging",
    "dev:prod": "electron-vite dev --mode production",
    "build": "npm run typecheck && electron-vite build",
    "build:prod": "npm run typecheck && electron-vite build --mode production",
    "build:staging": "npm run typecheck && electron-vite build --mode staging",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:win:prod": "npm run build:prod && electron-builder --config electron-builder.prod.yml --win",
    "build:win:staging": "npm run build:staging && electron-builder --config electron-builder.staging.yml --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:mac:prod": "npm run build:prod && electron-builder --config electron-builder.prod.yml --mac",
    "build:mac:staging": "npm run build:staging && electron-builder --config electron-builder.staging.yml --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:linux:prod": "npm run build:prod && electron-builder --config electron-builder.prod.yml --linux",
    "build:linux:staging": "npm run build:staging && electron-builder --config electron-builder.staging.yml --linux",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile build/smui-dark.css -i src/theme/dark"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@svar-ui/svelte-grid": "^2.3.0",
    "awesome-notifications": "^3.1.3",
    "axios": "^1.12.2",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.3.9",
    "file-saver": "^2.0.5",
    "jsbarcode": "^3.12.1",
    "jszip": "^3.10.1",
    "sv-router": "^0.10.1",
    "svelte-material-ui": "^8.0.3",
    "svelte-splitpanes": "^8.0.9",
    "tabulator-tables": "^6.3.1",
    "xlsx": "^0.18.5",
    "xstate": "^5.22.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.544.0",
    "@smui/tab": "^8.0.3",
    "@smui/tab-bar": "^8.0.3",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^25.0.3",
    "autoprefixer": "^10.4.21",
    "bits-ui": "^2.11.4",
    "clsx": "^2.1.1",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-svelte": "^3.11.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "smui-theme": "^8.0.3",
    "svelte": "^5.36.10",
    "svelte-check": "^4.3.4",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.0.5"
  }
}
